{
    "manifest_version": 2,
    "name": "UE QuickLaunch",
    "description": "Quick Access to links for UE",
    "version": "1.0.0",
    "icons": {"128": "icon.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}